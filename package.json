    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "all-contributors-cli": "^6.9.1",
    "husky": "^3.0.8",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "tsdx": "^0.9.3",
    "tslib": "^1.10.0",
    "typescript": "^3.6.3"
  }
