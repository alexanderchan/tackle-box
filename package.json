{
  "name": "tackle-box",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/tackle-box.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom",
    "lint": "tsdx lint"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
